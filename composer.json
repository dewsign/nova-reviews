{
    "name": "dewsign/nova-testimonials",
    "description": "A testimonials package for Laravel Nova",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Crewdson",
            "email": "danielcrewdson@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.0.0",
        "dewsign/nova-field-sortable": "^0.1.3",
        "maxfactor/support": "2.2.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "Dewsign\\NovaTestimonials\\": "src-php/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Dewsign\\NovaTestimonials\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
